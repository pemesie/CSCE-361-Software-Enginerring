<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="117.0" maxWidth="369.0" minHeight="117.0" minWidth="369.0" prefHeight="117.0" prefWidth="369.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.updatePassController">
   <top>
      <VBox prefHeight="75.0" prefWidth="543.0" BorderPane.alignment="CENTER">
         <children>
            <MenuBar />
            <Label prefHeight="17.0" prefWidth="169.0" text="Your password">
               <VBox.margin>
                  <Insets left="15.0" />
               </VBox.margin>
            </Label>
            <HBox spacing="10.0">
               <children>
                  <PasswordField fx:id="passField" prefHeight="25.0" prefWidth="223.0">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </PasswordField>
                  <Region />
                  <Label fx:id="credential" prefHeight="24.0" prefWidth="95.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </VBox.margin>
            </HBox>
            <Label fx:id="newPassLabel" layoutX="25.0" layoutY="10.0" prefHeight="17.0" prefWidth="220.0" text="Enter your new password">
               <VBox.margin>
                  <Insets left="15.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
   </top>
   <bottom>
      <HBox prefHeight="25.0" prefWidth="337.0" spacing="10.0" BorderPane.alignment="BOTTOM_LEFT">
         <children>
            <TextField fx:id="newPassField" prefHeight="25.0" prefWidth="222.0">
               <HBox.margin>
                  <Insets left="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <Button layoutX="308.0" layoutY="10.0" mnemonicParsing="false" onAction="#updateButtonPressed" prefHeight="25.0" prefWidth="79.0" text="update">
               <HBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </BorderPane.margin>
      </HBox>
   </bottom>
</BorderPane>
